<simulation>
  <archetypes>
    <spec>
      <lib>d3ploy.demand_fac</lib>
      <name>DemandFac</name>
    </spec>
    <spec>
      <lib>agents</lib>
      <name>NullRegion</name>
    </spec>
    <spec>
      <lib>d3ploy.deterministic_inst</lib>
      <name>DeterministicInst</name>
    </spec>
  </archetypes>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>POWER</supply_commod>
        <demand_commod>fuel</demand_commod>
        <demand_rate_max>2000.</demand_rate_max>
        <demand_rate_min>2000.</demand_rate_min>
        <supply_rate_max>1000.</supply_rate_max>
        <supply_rate_min>1000.</supply_rate_min>
      </DemandFac>
    </config>
    <name>Reactor</name>
  </facility>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>fuel</supply_commod>
        <demand_commod>uranium</demand_commod>
        <demand_rate_max>200000.</demand_rate_max>
        <demand_rate_min>200000.</demand_rate_min>
        <supply_rate_max>10000.</supply_rate_max>
        <supply_rate_min>10000.</supply_rate_min>
      </DemandFac>
    </config>
    <name>Enrichment</name>
  </facility>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>uranium</supply_commod>
        <demand_commod>x</demand_commod>
        <demand_rate_max>0.</demand_rate_max>
        <demand_rate_min>0.</demand_rate_min>
        <supply_rate_max>100000.</supply_rate_max>
        <supply_rate_min>100000.</supply_rate_min>
      </DemandFac>
    </config>
    <name>Mine</name>
  </facility>
  <region>
    <config>
      <NullRegion>
      </NullRegion>
    </config>
    <institution>
      <config>
        <DeterministicInst>
          <prototypes>
            <val>Reactor</val>
            <val>Enrichment</val>
            <val>Mine</val>
          </prototypes>
          <demand_eq>2*t</demand_eq>
          <fac_rates>
            <val>10,0,0</val>
            <val>10,-5,0</val>
            <val>10,0,-5</val>
          </fac_rates>
        </DeterministicInst>
      </config>
      <initialfacilitylist>
        <entry>
          <number>1</number>
          <prototype>Reactor</prototype>
        </entry>
        <entry>
          <number>1</number>
          <prototype>Enrichment</prototype>
        </entry>
        <entry>
          <number>1</number>
          <prototype>Mine</prototype>
        </entry>
      </initialfacilitylist>
      <name>ReactorInst</name>
    </institution>
    <name>SingleRegion</name>
  </region>
</simulation>
